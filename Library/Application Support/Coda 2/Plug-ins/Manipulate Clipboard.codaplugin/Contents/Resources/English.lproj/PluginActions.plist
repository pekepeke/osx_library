<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>PluginActions</key>
	<dict>
		<key>Swap With Clipboard</key>
		<dict>
			<key>action</key>
			<string>SwapClipboard</string>
			<key>shortcut</key>
			<string>^~x</string>
			<key>requireSelection</key>
			<true/>
			<key>submenu</key>
			<string>Clipboard</string>
		</dict>
		<key>Append To Clipboard</key>
		<dict>
			<key>action</key>
			<string>AppendClipboard</string>
			<key>shortcut</key>
			<string>^~c</string>
			<key>requireSelection</key>
			<true/>
			<key>submenu</key>
			<string>Clipboard</string>
		</dict>
	</dict>
</dict>
</plist>
